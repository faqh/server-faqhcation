<div class="tab-pane fade show active" id="showImage" role="tabpanel" aria-labelledby="home-tab">
    <!-- DataTales Example -->
    <div class="row">
        <% for(let i = 0; i < item.imageId.length; i++){ %>
        <div class="col-md-4">
            <div class="card shadow mb-4">
                <div class="card-body">
                        <img src="https://admin-faqhcation.herokuapp.com/<%= item.imageId[i].imageUrl %>" alt="image" style="width: 100%; display: block;">
                </div>
            </div>            
        </div>
        <% } %>
    </div>
    <a href="/admin/item" class="btn btn-warning btn-md">Back</a>
  </div>